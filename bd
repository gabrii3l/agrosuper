1) CREAR BASE DE DATOS LLAMADO ALMACEN

TABLA REGION:
 
	IDREGION (PK) INT AUTOINCREMENTAL.
	NOMBREREGION VARCHAR
	ESTADO char(1)
TABLA LOCALES:
 
	IDLOCALES (PK) INT AUTOINCREMENTAL.
	IDREGION (FK) INT,
	NOMBRELOCAL VARCHAR,
	ESTADO char(1)
TABLA TRABAJADOR:
 
	-IDTRABAJADOR (PK) AUTOINCREMENTAL
	-IDLOCAL INT (FK)
	-PNOMBRE VARCHAR
	-SNOMBRE VARCHAR
	-APATERNO VARCHAR
	-AMATERNO VARCHAR
	-EDAD INT

TABLA LIQUIDACION:
	-IDLIQUIDACION (PK) AUTOINCREMENTAL.
	-IDTRABAJADOR (FK),
	-TOTALHABERES INT,
	-TOTALDESCUENTOS INT,
	-TOTALLIQUIDO INT,
	-FECHAPAGO DATE.
TABLA PRODUCTO:
	IDPRODUCTO (PK) AUTOINCREMENTAL,
	NOMBREPRODUCTO VARCHAR,
	STOCK INT,
	FECHAVENCIMIENTO DATE.

TABLA VENTAS:
	IDVENTAS (PK) AUTOINCREMENTAL.
	IDTRABAJADOR (FK),
	IDPRODUCTO (FK),
	CANTIDAD INT,
	VALOR INT,
	FECHAVENTA DATE
