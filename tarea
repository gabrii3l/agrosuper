CREAR TABLA TIPOUNIDAD (IDTIPOUNIDAD INT PRIMARY KEY AUTOINCREMENTAL, NOMBREUNIDAD VARCHAR(50), ESTADO CHAR(1))

CREATE TABLE TIPOUNIDAD(
IDTIPOUNIDAD INT PRIMARY KEY IDENTITY(1,1),
NOMBREUNIDAD VARCHAR(100),
ESTADO CHAR(1)
);

INSERTE LOS SIGUIENTES TIPOS DE UNIDAD:

- KILO
- UNIDAD

INSERT INTO TIPOUNIDAD VALUES ('KILO', 'A');
INSERT INTO TIPOUNIDAD VALUES ('UNIDAD', 'A');

AGREGAR COLUMNA TIPO UNIDAD INT

ALTER TABLE PRODUCTO ADD IDTIPOUNIDAD INT

AGREGAR RESTRICCION CON TABLA TIPO UNIDAD

ALTER TABLE PRODUCTO ADD CONSTRAINT FK_TIPOUNIDADPRODUCTO FOREIGN KEY(IDTIPOUNIDAD)
REFERENCES TIPOUNIDAD(IDTIPOUNIDAD)


